<Container>:

    MDBottomNavigation:

        MDBottomNavigationItem:
            name: "screen1"
            text: "P"
            icon: "vector-point"
            BoxLayout:
                size_hint: 1,1
                orientation: "vertical"
                padding: 0
                spacing: 0
                BoxLayout:
                    padding: 0
                    size_hint: 1,0.02
                    orientation: "vertical"
                    MDScreen:
                        MDFlatButton:
                            size_hint: 0.05, 0.005
                            id: file
                            text: 'File'
                            pos_hint: {'center_x': 0.04, 'center_y': 0.1}
                            on_release: root.dropdown_file()

                        MDFlatButton:
                            size_hint: 0.05, 0.005
                            id: noise
                            text: 'Noise'
                            pos_hint: {'center_x': 0.124, 'center_y': 0.1}
                            on_release: root.dropdown_noise()



                BoxLayout:
                    padding: 0
                    size_hint: 1,0.8
                    orientation: "horizontal"
                    BoxLayout:
                        padding: 0
                        size_hint: 0.7,0.95
                        orientation: "vertical"
                        id: graph_p


                    BoxLayout:
                        padding: 0
                        size_hint: 0.3,1
                        orientation: "vertical"

                        BoxLayout:
                            padding: 0
                            size_hint: 1,0.2
                            orientation: "vertical"
                        BoxLayout:
                            padding: 0
                            size_hint: 1,0.1
                            orientation: "vertical"
                            padding: 15
                            Label:
                                text: f'Amplitude:  {round(ampl_p.value, 2)}'
                                font_size: 23
                                pos_hint: {'center_x': 0.18, 'center_y': 0.5}

                            MDSlider:

                                id: ampl_p
                                min: -1
                                max: 1
                                value: root.get_default_slider_value('P', 0)
                                step: 0.01
                                hint: False
                                on_value: root.update_plot('P', 0, ampl_p.value)
                        BoxLayout:
                            padding: 0
                            size_hint: 1,0.1
                            orientation: "vertical"
                            padding: 15
                            Label:
                                text: f"Time: {round(time_p.value, 3)}"
                                font_size: 23
                                pos_hint: {'center_x': 0.12, 'center_y': 0.5}

                            MDSlider:
                                id: time_p
                                min: 0
                                max: 1
                                value: root.get_default_slider_value('P', 3)
                                step: 0.00001
                                hint: False
                                on_value: root.update_plot('P', 3, time_p.value)

                        BoxLayout:
                            padding: 0
                            size_hint: 1,0.1
                            orientation: "vertical"
                            padding: 15
                            Label:
                                text: f"b1:  {round(b1_p.value, 3)}"
                                font_size: 23
                                pos_hint: {'center_x': 0.1, 'center_y': 0.5}
                            MDSlider:
#                                thumb_color_active: [0,0,0,1]

                                id: b1_p
                                min: 0.000001
                                max: 0.1
                                value: root.get_default_slider_value('P', 1)
                                step: 0.0001
                                hint: False
                                on_value: root.update_plot('P', 1, b1_p.value)


                        BoxLayout:
                            padding: 0
                            size_hint: 1,0.1
                            orientation: "vertical"
                            padding: 15
                            Label:
                                text: f"b2:  {round(b2_p.value, 3)}"
                                font_size: 23
                                pos_hint: {'center_x': 0.1, 'center_y': 0.5}
                            MDSlider:
    #                            thumb_color_active: [0,0,0,1]
                                id: b2_p
                                min: 0.00001
                                max: 0.02
                                value: root.get_default_slider_value('P', 2)
                                step: 0.0001
                                hint: False
                                on_value: root.update_plot('P', 2, b2_p.value)


                        BoxLayout:
                            padding: 0
                            size_hint: 0.45,0.2
                            orientation: "vertical"
                            padding: 50
                            MDTextField:
                                hint_text: "HR"
                                mode: "rectangle"
                                text: "60"


                        BoxLayout:
                            padding: 0
                            size_hint: 1,0.3
                            orientation: "vertical"
                            padding: 15


